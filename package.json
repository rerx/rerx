{
  "name": "rerx",
  "version": "0.2.0",
  "description": "Reactive React with RxJS",
  "scripts": {
    "clean": "rimraf core component utils",
    "test": "BABEL_ENV=test ava --verbose",
    "build": "npm run clean && webpack --display-modules --progress",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rerx/rerx.git"
  },
  "files": [
    "core",
    "component",
    "utils",
    "src"
  ],
  "author": "Jas Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rerx/rerx/issues"
  },
  "homepage": "https://github.com/rerx/rerx",
  "peerDependencies": {
    "rxjs": "^5.0.0-beta.10",
    "react": "^0.14.0 || ^15.0.0-0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "jsdom": "^9.4.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.2",
    "rxjs": "^5.0.0-beta.10",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
